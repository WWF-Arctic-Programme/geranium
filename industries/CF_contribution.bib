@article{RN331,
   author = {Johannesen, E. and Hoines, A. S. and Dolgov, A. V. and Fossheim, M.},
   title = {Demersal fish assemblages and spatial diversity patterns in the Arctic-Atlantic transition zone in the Barents Sea},
   journal = {PLoS One},
   volume = {7},
   number = {4},
   pages = {e34924},
   note = {Johannesen, Edda
Hoines, Age S
