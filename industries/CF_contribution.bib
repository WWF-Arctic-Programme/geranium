@article{RN22,
   author = {Merkel, B. and Lydersen, C. and Yoccoz, N. G. and Kovacs, K. M.},
   title = {The world's northernmost harbour seal population-how many are there?},
   journal = {PLoS One},
   volume = {8},
   number = {7},
   pages = {e67576},
   note = {Merkel, Benjamin
Lydersen, Christian
Yoccoz, Nigel G
Kovacs, Kit M
eng
Research Support, Non-U.S. Gov't
2013/07/12
PLoS One. 2013 Jul 3;8(7):e67576. doi: 10.1371/journal.pone.0067576. Print 2013.},
   abstract = {This study presents the first abundance estimate for the world's northernmost harbour seal (Phoca vitulina) population, which resides in Svalbard, Norway, based on three digital stereoscopic photographic surveys conducted in 2009 and 2010. The counts from these high resolution 3D images were combined with a novel method for estimating correction factors for animals that were in the water at the time of the surveys, in which extensive behavioural data from radio-tagged harbour seals were used together with age distribution data to estimate the proportion of seals of various age and sex classes hauled out at the times of the surveys. To detect possible seasonal shifts in age distribution between surveys, lengths of hauled out seals were measured from the stereoscopic images. No body-length differences were detected between the surveys; but, this may be due to a high degree of sexual dimorphism exhibited in this population. Applying the modelled correction factors, a total of 1888 (95% CI: 1660-3023), 1742 (1381-3549) and 1812 (1656-4418) harbour seals were estimated for the surveys flown on 01 August 2009, 01 August 2010 and 19 August 2010, respectively. The similarity between the three survey estimates (despite significant differences in the number of animals actually counted on the photos from each survey effort) suggests that the variation in numbers of hauled out seals is reasonably accurately adjusted for by the haul-out probability model. The low population size, the limited spatial distribution of the population and its reduced genetic diversity make this population vulnerable to chance events, such as disease epidemics.},
   keywords = {Age Factors
Analysis of Variance
Animals
Female
Male
Models, Statistical
Norway
*Phoca
Population Density},
   ISSN = {1932-6203 (Electronic)
1932-6203 (Linking)},
   DOI = {10.1371/journal.pone.0067576},
   url = {https://www.ncbi.nlm.nih.gov/pubmed/23844035},
   year = {2013},
   type = {Journal Article}
}

